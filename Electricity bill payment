#TELEPHONE BILLING:
import random
unit=int
price=12      #per unit
units=int(input('enter the number of unit:'))
reading=units*price
result=print('your price for electricty bill is',reading)
print(result)

#   pay or not for monthly 100units:
if units==100:
    print('you will not pay the amount because your reading unit is less than 100')
if units<100:
    print('you will not pay the amount')
elif units>100:
    print('u will pay the amount for this month electricity bill',reading)
else:
    print('THANKS FOR ELECTRICITY BOARD')
import datetime
datetime=datetime.datetime.now()
print(datetime,result)
